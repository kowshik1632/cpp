#include<iostream>
using namespace std;
int main()
{
	int age;
	cout<<"Enter the Age of the user: ";
	cin>>age;
	
	if(age>=20)
	{
		cout<<"\nYou are major.\nYou are eligible to vote.";
	}
	else if(age>=18)
	{
		cout<<"\nYou are minor.\nYou are eligible to vote.";
	}
	else
		cout<<"\n You are not eligible to vote";
return 0;	
}
